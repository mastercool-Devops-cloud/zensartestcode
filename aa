@Test
    public void testDoFilter_Success() throws Exception {
        // Arrange
        when(mockRequest.getAttribute("startTime")).thenReturn(System.currentTimeMillis());

        ArgumentCaptor<ServletResponse> responseCaptor = ArgumentCaptor.forClass(ServletResponse.class);

        // Act
        responseLogFilter.doFilter(mockRequest, mockResponse, mockFilterChain);

        // Assert
        verify(mockFilterChain).doFilter(eq(mockRequest), responseCaptor.capture());

        // Ensure it is an instance of ContentCachingResponseWrapper
        assertTrue(responseCaptor.getValue() instanceof ContentCachingResponseWrapper);
    }
