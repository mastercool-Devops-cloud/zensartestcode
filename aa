 @GetMapping("/add_client")
	  public Long add_client(@RequestParam String p_input_clientno,@RequestParam String p_input_buy_sell,@RequestParam String p_ccy_code,@RequestParam String p_SSI_req) {
		  String fx_Client_Info_URL = "http://localhost:8084/fx-client/findcif/" + p_input_clientno;
		  FxClientInfoDTO fxClientInfoDTO= restTemplate.getForObject(fx_Client_Info_URL, FxClientInfoDTO.class);
			return fxClientInfoDTO.getCifCustN();
